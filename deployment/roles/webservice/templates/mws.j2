server {
  listen 80 default_server;
  server_name _;
  access_log /var/log/nginx/mws.log;

  location /dist/ {
      root /opt/mws/mongo-web-shell/frontend;
      if ($query_string) {
           expires max;
      }
  }

  location /mws {
    include uwsgi_params;
    uwsgi_pass unix:/tmp/mws-uwsgi.sock;
  }

  location /ivs {
    include uwsgi_params;
    uwsgi_pass unix:/tmp/ivs-uwsgi.sock;
  }

  location /ivp {
    include uwsgi_params;
    uwsgi_pass unix:/tmp/ivp-uwsgi.sock;
  }
}